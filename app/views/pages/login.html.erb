<div class="container">
<h1>Log In</h1>

<%= form_with url: login_path do |form|%>
  <div class= "mb-3"> 
    <%= form.label :email %>
    <%= form.text_field :email, class: "form-control", placeholder: "Email"%>
  </div>
    <div class= "mb-3"> 
    <%= form.label :password %>
    <%= form.password_field :password, class: "form-control", placeholder: "Password"%>
  </div>
   
  <div class= "mb-3"> 
    <%= form.submit "Login", class: "btn btn-primary" %>
    <%= link_to "Home", home_path, class: "btn btn-success" %>
  </div>
<% end %>
</div>
<%= link_to 'home', home_path %>